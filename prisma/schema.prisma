// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

model User {
  id          Int    @id @default(autoincrement())
  external_Id String
  username    String
}

model Repo {
  id          Int    @id @default(autoincrement())
  private_key String
  public_key  String
  name        String
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
